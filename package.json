{
  "name": "sigma-icons",
  "displayName": "Sigma",
  "description": "File icons and syntax for Sigma programming language",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "sigma",
      "aliases": ["Sigma", "sigma"],
      "extensions": [".sgm"],
      "configuration": "./language-configuration.json",
      "icon": {
        "light": "./icons/sigma.svg",
        "dark": "./icons/sigma.svg"
      }
    }],
    "grammars": [{
      "language": "sigma",
      "scopeName": "source.sigma",
      "path": "./syntaxes/sigma.tmLanguage.json"
    }]
  }
}
